#!/bin/bash
usage() {
  cat << EOF
  wal-discord - generate custom discord css files based on pywal colors
    -h  Display this help.
    -s  Output result to stdout. useful with xclip for usage with betterdiscord.
    -b  Select backend to use. 
        Supported backends are \'wal', \'haishoku'. Default is \'wal'.
EOF
exit 0
}

init() {
  
}


